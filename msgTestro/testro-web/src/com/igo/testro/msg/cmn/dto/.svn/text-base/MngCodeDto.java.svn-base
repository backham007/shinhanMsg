package com.igo.testro.msg.cmn.dto;

import com.igo.testro.dto.AbstractDTO;

public class MngCodeDto extends AbstractDTO{
	
	private static final long serialVersionUID = 1L;
	
	private String cdclMnName;     //구분코드명
	private String dtailCd;        //상세 value
	private String divName;		   //구분코드 명
	private String dtailName;      //상세 text
	private String sidx;           //oderby key 값
	private String sord;		   //오름차(asc), 내림차(desc)
	private String useYN;          //사용유무(Y: 사용 N: 미사용)
	
	
	public String getUseYN() {
		return useYN;
	}
	public void setUseYN(String useYN) {
		this.useYN = useYN;
	}
	public String getSidx() {
		return sidx;
	}
	public void setSidx(String sidx) {
		this.sidx = sidx;
	}
	public String getSord() {
		return sord;
	}
	public void setSord(String sord) {
		this.sord = sord;
	}
	public String getCdclMnName() {
		return cdclMnName;
	}
	public void setCdclMnName(String cdclMnName) {
		this.cdclMnName = cdclMnName;
	}
	public String getDtailCd() {
		return dtailCd;
	}
	public void setDtailCd(String dtailCd) {
		this.dtailCd = dtailCd;
	}
	public String getDivName() {
		return divName;
	}
	public void setDivName(String divName) {
		this.divName = divName;
	}
	public String getDtailName() {
		return dtailName;
	}
	public void setDtailName(String dtailName) {
		this.dtailName = dtailName;
	}
	
	
	
	
}
