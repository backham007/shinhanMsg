package com.igo.testro.cmn.service.excel;

public class ExcelAttributeDTO {
	
	// 필드명
	private String fieldNm;
	
	// 데이터타입
	private int type;
	
	// 정렬
	private short align;
	
	// 헤더명
	private String headerNm;
	
	// 헤더포함 가장긴 문장길이
	private int columnMaxLangth = 0;
	

	/**
	 * @return align
	 */
	public short getAlign() {
		return align;
	}

	/**
	 * @param align 설정하려는 align
	 */
	public void setAlign(short align) {
		this.align = align;
	}

	/**
	 * @return headerNm
	 */
	public String getHeaderNm() {
		return headerNm;
	}

	/**
	 * @param headerNm 설정하려는 headerNm
	 */
	public void setHeaderNm(String headerNm) {
		this.headerNm = headerNm;
	}

	/**
	 * @return type
	 */
	public int getType() {
		return type;
	}

	/**
	 * @param type 설정하려는 type
	 */
	public void setType(int type) {
		this.type = type;
	}

	/**
	 * @return fieldNm
	 */
	public String getFieldNm() {
		return fieldNm;
	}

	/**
	 * @param fieldNm 설정하려는 fieldNm
	 */
	public void setFieldNm(String fieldNm) {
		this.fieldNm = fieldNm;
	}

	public int getColumnMaxLangth() {
		return columnMaxLangth;
	}

	public void setColumnMaxLangth(int columnMaxLangth) {
		if (this.columnMaxLangth < columnMaxLangth) {
			this.columnMaxLangth = columnMaxLangth;
		}
	}
}
